<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="169.0" prefWidth="473.0" style="-fx-background-color: #10403B;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="connection.seller.signin.pin.ConfirmAdminController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Admin PIN :" textFill="#4c5958">
         <font>
            <Font name="Berlin Sans FB" size="30.0" />
         </font>
      </Label>
      <PasswordField fx:id="pinField" layoutX="162.0" layoutY="85.0" promptText="PIN" />
      <Label fx:id="pinLabel" layoutX="179.0" layoutY="58.0" textFill="RED" />
      <Label fx:id="invalidLabel" layoutX="207.0" layoutY="58.0" textFill="RED" />
      <Button id="green" fx:id="confirmButton" layoutX="183.0" layoutY="118.0" mnemonicParsing="false" onAction="#confirm" prefHeight="26.0" prefWidth="106.0" stylesheets="@stylesheet.css" text="Confirm" />
      <Button id="green" fx:id="cancelButton" layoutX="400.0" layoutY="14.0" mnemonicParsing="false" onAction="#cancel" prefHeight="5.0" prefWidth="58.0" stylesheets="@stylesheet.css" text="X">
         <font>
            <Font name="Berlin Sans FB Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
